{
  "plugins": ["plugins/markdown"],
  "recurseDepth": 10,
  "source": {
    "include": ["project-info.js", "rollup.config.js","tools"],
    "includePattern": "\\.m?js$",
    "excludePattern": "(node_modules/|jsdoc|doc|\\.test\\.js)"
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "opts": {
    "destination": "./jsdoc-tools",
    "recurse": true,
    "readme": "./readme.md"
  }
}